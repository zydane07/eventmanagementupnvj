<div class="wrapper">
    <!-- Preloader -->
    <div class="preloader flex-column justify-content-center align-items-center">
        <img class="animation__shake" src="/img/logo.png" alt="AdminLTELogo" height="60" width="60" />
    </div>
    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <div class="content-header">
            <div class="container-fluid">
                <div class="row mb-2">
                    <div class="col-sm-6">
                        <h1 class="m-0">Dashboard</h1>
                    </div>
                    <!-- /.col -->
                    <div class="col-sm-6">
                        <ol class="breadcrumb float-sm-right">
                            <li class="breadcrumb-item"><a href="#">Home</a></li>
                            <li class="breadcrumb-item active">Dashboard</li>
                        </ol>
                    </div>
                    <!-- /.col -->
                </div>
                <!-- /.row -->
            </div>
            <!-- /.container-fluid -->
        </div>
        <!-- /.content-header -->

        <!-- Main content -->
        <div class="content">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-4 col-12">
                        <!-- small box -->
                        <div class="small-box bg-lightblue">
                            <div class="inner">
                                <% if (typeof jumlahMhs != 'undefined') { %>
                                    <h3><%= jumlahMhs %></h3>
                                
                                <% } else { %>
                                    <h3>0</h3>
                                <% } %>
                                <p>Mahasiswa Terdaftar</p>
                            </div>
                            <div class="icon">
                                <i class="ion ion-bag"></i>
                            </div>
                            <a href="#" class="small-box-footer">Lihat Detail<i class="fas fa-arrow-circle-right"></i></a>
                        </div>
                    </div>
                    <!-- ./col -->
                    <div class="col-lg-4 col-12">
                        <!-- small box -->
                        <div class="small-box bg-fuchsia">
                            <div class="inner">
                                <% if (typeof jumlahMhs != 'undefined') { %>
                                    <h3><%= jumlahOrm %></h3>
                                
                                <% } else { %>
                                    <h3>0</h3>
                                <% } %>
                                <p>Ormawa Terdaftar</p>
                            </div>
                            <div class="icon">
                                <i class="ion ion-stats-bars"></i>
                            </div>
                            <a href="#" class="small-box-footer">Lihat Detail <i class="fas fa-arrow-circle-right"></i></a>
                        </div>
                    </div>
                    <!-- ./col -->
                    <div class="col-lg-4 col-12">
                        <!-- small box -->
                        <div class="small-box bg-teal">
                            <div class="inner">
                                <% if (typeof jumlahMhs != 'undefined') { %>
                                    <h3><%= jumlahEvent2.length %></h3>
                                <% } else { %>
                                    <h3>0</h3>
                                <% } %>

                                <p>Event Tersedia</p>
                            </div>
                            <div class="icon">
                                <i class="ion ion-person-add"></i>
                            </div>
                            <a href="#" class="small-box-footer">Lihat Detail <i class="fas fa-arrow-circle-right"></i></a>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-6">
                        <div class="card card-primary card-outline">
                            <div class="card-header">
                                <h3 class="card-title">
                                    <i class="far fa-chart-bar"></i>
                                    Statistik Event berdasarkan Kategori
                                </h3>

                                <div class="card-tools">
                                    <button type="button" class="btn btn-tool" data-card-widget="collapse">
                                        <i class="fas fa-minus"></i>
                                    </button>
                                    <!-- <button type="button" class="btn btn-tool" data-card-widget="remove">
                                        <i class="fas fa-times"></i>
                                    </button> -->
                                </div>
                            </div>
                            
                            <div class="card-body">
                                <div id="donut-chart" style="height: 300px; padding: 0px; position: relative">
                                    <canvas class="flot-base" width="830" height="600" style="direction: ltr; position: absolute; left: 0px; top: 0px; width: 415px; height: 300px"></canvas
                                    ><canvas class="flot-overlay" width="830" height="600" style="direction: ltr; position: absolute; left: 0px; top: 0px; width: 415px; height: 300px"></canvas
                                    ><span class="pieLabel" id="pieLabel0" style="position: absolute; top: 69.8px; left: 265.456px"
                                        ><div style="font-size: 13px; text-align: center; padding: 2px; color: #fff; font-weight: 600">Series2<br />30%</div></span
                                    ><span class="pieLabel" id="pieLabel1" style="position: absolute; top: 209.8px; left: 243.456px"
                                        ><div style="font-size: 13px; text-align: center; padding: 2px; color: #fff; font-weight: 600">Series3<br />20%</div></span
                                    ><span class="pieLabel" id="pieLabel2" style="position: absolute; top: 128.8px; left: 84.4562px"
                                        ><div style="font-size: 13px; text-align: center; padding: 2px; color: #fff; font-weight: 600">Series4<br />50%</div></span
                                    >
                                </div>
                            </div>
                            <!-- /.card-body-->
                        </div>
                        <!-- /.card -->
                    </div>
                    <div class="col-lg-6">
                        <div class="card">
                            <div class="card-header border-0">
                                <h3 class="card-title">Event Tersedia</h3>
                                <div class="card-tools">
                                    <div id="example1_filter" class="dataTables_filter">
                                        <label><input type="search" class="form-control form-control-sm" placeholder="Cari Event" aria-controls="example1" /></label>
                                    </div>
                                </div>
                            </div>
                            <div class="card-body table-responsive p-0">
                                <table class="table table-striped table-valign-middle">
                                    <thead>
                                        <tr>
                                            <th style="width: 35%">Nama Event</th>
                                            <th style="width: 35%">Ormawa Penyelenggara</th>
                                            <th style="width: 23%">Tanggal Event</th>
                                            <!-- <th>Jumlah Pendaftar</th> -->
                                            <th style="width: 7%">Pendaftar</th>
                                            <!-- <th>More</th> -->
                                        </tr>
                                    </thead>
                                    <tbody>
                                        
                            <% jumlahEvent2.forEach(event =>{ %>
                                <tr>
                                    <td>
                                        <a><%= event.nama_event %></a>
                                        <br />
                                        <!-- <medium> Created 01.01.2019 </medium> -->
                                    </td>
                                    <td><%=  event.detil_eo %></td>
                                    <td><%=  event.tanggal_event.toISOString().split('T')[0] %></td>
                
                                    <td class="project_progress">
                                         <medium> <%= event.registered_people.length %> Peserta</medium>
                                    </td>
                                </tr> 
                             
                            <% }) %> 
                                    </tbody>
                                </table>
                            </div>
                        </div>
                        <!-- /.card -->
                    </div>
                </div>
                <!-- <div class="row">
                
                </div> -->

                <!-- /.row -->
            </div>
            <!-- /.container-fluid -->
        </div>
        <!-- /.content -->
    </div>
    <!-- /.content-wrapper -->

    <!-- Control Sidebar -->
    <aside class="control-sidebar control-sidebar-dark">
        <!-- Control sidebar content goes here -->
    </aside>
    <!-- /.control-sidebar -->

    <!-- Main Footer -->
    <footer class="main-footer">
        <strong>Copyright &copy; 2014-2021 <a href="https://adminlte.io">AdminLTE.io</a>.</strong>
        All rights reserved.
        <div class="float-right d-none d-sm-inline-block"><b>Version</b> 3.2.0-rc</div>
    </footer>
</div>
