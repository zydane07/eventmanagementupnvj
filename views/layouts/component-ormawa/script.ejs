<% if (typeof jmlWebinar !== 'undefined') { %>
<script>
        $(function () {
      /*
       * DONUT CHART
       * -----------
       */

      var donutData = [
          {
              label: "Webinar",
              data: <%= jmlWebinar %>,
              color: "#3c8dbc",
          },
          {
              label: "Workshop",
              data: <%= jmlWorkshop %>,
              color: "#0073b7",
          },
          {
              label: "Lomba",
              data: <%= jmlLomba %>,
              color: "#00c0ef",
          },
          {
              label: "Lainnya",
              data: <%= jmlLainnya %>,
              color: "#0490b3",
          },
      ];
      $.plot("#donut-chart", donutData, {
          series: {
              pie: {
                  show: true,
                  radius: 1,
                  innerRadius: 0.5,
                  label: {
                      show: true,
                      radius: 2 / 3,
                      formatter: labelFormatter,
                      threshold: 0.1,
                  },
              },
          },
          legend: {
              show: false,
          },
      });
      /*
       * END DONUT CHART
       */
    });

    /*
    * Custom Label formatter
    * ----------------------
    */
    function labelFormatter(label, series) {
      return '<div style="font-size:13px; text-align:center; padding:2px; color: #fff; font-weight: 600;">' + label + "<br>" + Math.round(series.percent) + "%</div>";
    }
</script>
<% } %>

